{
    "keyboard.dispatch": "keyCode",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.editor.enablePreview": false,

    // Filespecific spacings
    "[ruby]": {
        "editor.tabSize": 2
    },

    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/venv/*/**": true
      }
}